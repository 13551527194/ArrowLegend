box, Game.hero.hbox);\r\n            this.pro.rotation(a);\r\n            this.gi.show = true;\r\n            this.gi.drawMoveline();\r\n        }\r\n    }\r\n    hit(pro) {\r\n        super.hit(pro);\r\n        if (this.pro.gamedata.hp <= 0) {\r\n            this.gi.show = false;\r\n        }\r\n    }\r\n}\r\n",
