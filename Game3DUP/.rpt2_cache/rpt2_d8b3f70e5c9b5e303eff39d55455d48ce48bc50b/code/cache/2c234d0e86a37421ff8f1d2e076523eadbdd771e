               minss = \"\" + minst + \"分\";\r\n                    }\r\n                    return hourss + minss;\r\n                }\r\n            }\r\n            else {\r\n                dayss = days + \"天\";\r\n                if (hourst == 0) {\r\n                    hourss = \"\";\r\n                }\r\n                else {\r\n                    if (hourst < 10)\r\n                        hourss = \"0\" + hourst + \"小时\";\r\n                    else\r\n                        hourss = \"\" + hourst + \"小时\";\r\n                    ;\r\n                }\r\n                return dayss + hourss;\r\n            }\r\n        }\r\n        return \"\";\r\n    }\r\n}\r\n",
